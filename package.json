  "type": "module",
  "main": "out/main/index.js",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build && electron-builder",
    "preview": "electron-vite preview",
    "start": "electron-vite preview --skipBuild",
    "tsc:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "tsc:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "tsc": "pnpm tsc:node && pnpm tsc:web",
    "test": "vitest run",
    "lint": "eslint .",
    "postinstall": "node scripts/postinstall.js"
  },
  "dependencies": {
    "electron-updater": "^6.6.2"
  },
  "devDependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@electron-toolkit/utils": "^4.0.0",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.17",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "clsx": "^2.1.1",
    "devtools-protocol": "^0.0.1550230",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "electron-log": "^5.4.3",
    "electron-vite": "^4.0.1",
    "eslint": "^9.39.1",
    "express": "^5.1.0",
    "p-queue": "^9.0.1",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.2.4",
    "vitest": "^4.0.14",
    "zod": "^4.1.13",
    "zustand": "^5.0.8"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  }
